<?xml version="1.0" encoding="utf-8"?>
<!--
-->
<favorites>
<!-- hotseat -->
    <!-- Dialer -->
    <favorite packageName="com.android.contacts" className="com.android.contacts.activities.DialtactsActivity"
        container="-101" screen="0" x="0" y="0"/>
    <!-- Contacts -->
    <favorite packageName="com.android.contacts" className="com.android.contacts.activities.PeopleActivity"
        container="-101" screen="1" x="0" y="0"/>
    <!-- Messaging -->
    <favorite packageName="com.google.android.apps.messaging" className="com.google.android.apps.messaging.ui.ConversationListActivity"
        container="-101" screen="2" x="0" y="0"/>
    <!-- Browser -->
    <favorite packageName="com.android.chrome" className="com.google.android.apps.chrome.Main"
        container="-101" screen="3" x="0" y="0"/>
    <!-- Camera -->
    <favorite packageName="com.evenwell.camera2" className="com.android.camera.CameraLauncher"
        container="-101" screen="4" x="0" y="0"/>

<!-- Center screen (1) -->
		
		<!-- Google Search Widget -->
    <widget packageName="com.google.android.googlequicksearchbox" className="com.google.android.googlequicksearchbox.SearchWidgetProvider"
        container="-100" screen="0" x="0" y="0" spanX="4" spanY="1"/>

		<folder container="-100" screen="0" x="0" y="4" title="Google">
        <!-- Google Search -->
        <favorite packageName="com.google.android.googlequicksearchbox" className="com.google.android.googlequicksearchbox.SearchActivity"/>
        <!-- Gmail -->
        <favorite packageName="com.google.android.gm" className="com.google.android.gm.ConversationListActivityGmail"/>
        <!-- Maps -->
        <favorite packageName="com.google.android.apps.maps" className="com.google.android.maps.MapsActivity"/>
        <!-- YouTube -->
        <favorite packageName="com.google.android.youtube" className="com.google.android.youtube.app.honeycomb.Shell$HomeActivity"/>
        <!-- Google Drive -->
        <favorite packageName="com.google.android.apps.docs" className="com.google.android.apps.docs.app.NewMainProxyActivity"/>
        <!-- Google Play Music -->
        <favorite packageName="com.google.android.music" className="com.android.music.activitymanagement.TopLevelActivity"/>
        <!-- Google Play Movies -->
        <favorite packageName="com.google.android.videos" className="com.google.android.youtube.videos.EntryPoint"/>
        <!-- Google Duo1 -->
        <favorite packageName="com.google.android.apps.tachyon" className="com.google.android.apps.tachyon.MainActivity"/>
        <!-- Google Photos -->
        <favorite packageName="com.google.android.apps.photos" className="com.google.android.apps.photos.home.HomeActivity"/>
        <!-- Voice Search -->
        <favorite packageName="com.google.android.googlequicksearchbox" className="com.google.android.googlequicksearchbox.VoiceSearchActivity"/>
    </folder>

    <!-- play Store -->
    <favorite packageName="com.android.vending" className="com.android.vending.AssetBrowserActivity"
        container="-100" screen="0" x="3" y="4"/>

<!-- Right1 screen (2) -->
    <folder container="-100" screen="1" x="0" y="0" title="系统工具" key="SYSTEM_TOOL">
        <!-- Calendar -->
        <favorite packageName="com.google.android.calendar" className="com.android.calendar.AllInOneActivity"/>
        <!-- DeskClock -->
        <favorite packageName="com.google.android.deskclock" className="com.android.deskclock.DeskClock"/>
        <!-- Calculator -->
        <favorite packageName="com.android.calculator2" className="com.android.calculator2.Calculator"/>
        <!-- Download -->
        <favorite packageName="com.android.documentsui" className="com.android.documentsui.LauncherActivity"/>
        <!-- Setting -->
        <favorite packageName="com.android.settings" className="com.android.settings.Settings"/>
    </folder>

